<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sean.assignment2" >

    <uses-permission android:name="android.permission.WRITE_SETTINGS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Math Game"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Level1_Activity"
            android:label="@string/title_activity_level1_"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".Level2_Activity"
            android:label="@string/title_activity_level2_"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".Level3_Activity"
            android:label="@string/title_activity_level3_"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".Finish_Activity"
            android:label="@string/title_activity_finish_"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
    </application>

</manifest>
